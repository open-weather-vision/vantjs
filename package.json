{
    "name": "vantjs",
    "version": "2.1.0",
    "description": "platform independent javascript/typescript interface to Davis Vantage Pro, Pro 2 and Vue",
    "exports": {
        ".": "./dist/index.js",
        "./errors": "./dist/errors/index.js",
        "./weather-station": "./dist/weather-station/index.js",
        "./weather-station/settings": "./dist/weather-station/settings/index.js",
        "./weather-station/events": "./dist/weather-station/events/index.js",
        "./realtime-containers": "./dist/realtime-containers/index.js",
        "./realtime-containers/settings": "./dist/realtime-containers/settings/index.js",
        "./realtime-containers/events": "./dist/realtime-containers/events/index.js",
        "./structures": "./dist/structures/index.js",
        "./structures/subtypes": "./dist/structures/subtypes/index.js",
        "./units": "./dist/units/index.js",
        "./util": "./dist/util/index.js"
    },
    "typesVersions": {
        "*": {
            "errors": [
                "dist/errors"
            ],
            "weather-station": [
                "dist/weather-station"
            ],
            "weather-station/settings": [
                "dist/weather-station/settings"
            ],
            "weather-station/events": [
                "dist/weather-station/events"
            ],
            "realtime-containers": [
                "dist/realtime-containers"
            ],
            "realtime-containers/settings/": [
                "dist/realtime-containers/settings"
            ],
            "realtime-containers/events": [
                "dist/realtime-containers/events"
            ],
            "structures": [
                "dist/structures"
            ],
            "structures/subtypes": [
                "dist/structures/subtypes"
            ],
            "units": [
                "dist/units"
            ],
            "util": [
                "dist/util"
            ]
        }
    },
    "types": "dist/index.d.ts",
    "type": "module",
    "scripts": {
        "build": "npx tsc",
        "watch": "npx tsc --watch",
        "start": "cd ./dist && node ",
        "docs": "typedoc --options typedoc.json",
        "watch-docs": "typedoc --options typedoc.json --watch",
        "clear-dist": "rmdir /s dist && npx tsc"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/harrydehix/vantjs.git"
    },
    "keywords": [
        "davis vantage pro",
        "davis vantage vue",
        "davis vantage pro 2",
        "vantjs",
        "javascript vantage",
        "driver",
        "interface"
    ],
    "author": "Henri Hollmann",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/harrydehix/vantjs/issues"
    },
    "homepage": "https://github.com/harrydehix/vantjs#readme",
    "dependencies": {
        "@harrydehix/easy-buffer": "^1.2.1",
        "crc-full": "^1.1.0",
        "deep-for-each": "^3.0.0",
        "lodash.clonedeep": "^4.5.0",
        "lodash.flow": "^3.5.0",
        "lodash.merge": "^4.6.2",
        "lodash.mergewith": "^4.6.2",
        "serialport": "^10.4.0",
        "source-map-support": "^0.5.21",
        "sprintf-js": "^1.0.3",
        "tiny-typed-emitter": "^2.1.0",
        "vant-environment": "^4.0.1"
    },
    "devDependencies": {
        "@types/lodash.clonedeep": "^4.5.6",
        "@types/lodash.flow": "^3.5.6",
        "@types/lodash.merge": "^4.6.6",
        "@types/lodash.mergewith": "^4.6.6",
        "@types/node": "^15.14.9",
        "@types/serialport": "^8.0.2",
        "@types/sprintf-js": "^1.1.2",
        "ts-node": "^10.0.0",
        "typedoc": "^0.25.1",
        "typescript": "^5.2.2"
    },
    "eslintConfig": {
        "rules": {
            "no-case-declarations": 0,
            "@typescript-eslint/no-case-declarations": 0
        }
    }
}